# debug.log

import sys
import logging

def redirect_output_to_debug_log():
    debug_log_file = open("debug.log", "w")
    sys.stdout = debug_log_file
    sys.stderr = debug_log_file

def configure_logging():
    logging.basicConfig(filename="debug.log", level=logging.DEBUG)

# Redirect output to debug.log
redirect_output_to_debug_log()

# Configure logging
configure_logging()
